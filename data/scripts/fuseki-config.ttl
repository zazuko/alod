@prefix :        <#> .
@prefix fuseki:  <http://jena.apache.org/fuseki#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix tdb:     <http://jena.hpl.hp.com/2008/tdb#> .
@prefix ja:      <http://jena.hpl.hp.com/2005/11/Assembler#> .

[] rdf:type fuseki:Server;
	fuseki:services (
		<#alodService>
	).

<#alodService> rdf:type fuseki:Service;
	fuseki:name                        "alod";
	fuseki:serviceQuery                "sparql";
	fuseki:serviceQuery                "query";
	fuseki:serviceUpdate               "update";
	fuseki:serviceUpload               "upload";
	fuseki:serviceReadWriteGraphStore  "data";
	fuseki:serviceReadGraphStore       "get";
	fuseki:dataset                     <#alodDataset>;
	.

<#alodDataset> rdf:type ja:RDFDataset;
	ja:namedGraph [
		ja:graphName	<http://localhost:3030/alod/bar>;
		ja:graph [ a ja:MemoryModel;
			ja:content [
				ja:externalContent <file:../graphs/bar.ttl>
			];
		];
	];
	ja:namedGraph [
		ja:graphName	<http://localhost:3030/alod/ne>;
		ja:graph [ a ja:MemoryModel;
			ja:content [
				ja:externalContent <file:../graphs/ne.ttl>
			];
		];
	];
	.