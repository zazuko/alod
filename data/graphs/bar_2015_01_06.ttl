	alod:volume "1620.0" ;
	time:intervalStarts "1801"^^<http://www.w3.org/2001/XMLSchema#gYear> ;
	time:intervalEnds "1801"^^<http://www.w3.org/2001/XMLSchema#gYear> ;
	locah:maintenanceAgencyCode "CH-000018-2" ;
